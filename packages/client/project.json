{
  "root": "packages/client",
  "sourceRoot": "packages/client/src",
  "targets": {
    "build": {
      "executor": "./tools/executors/lib-builder:run-build",
      "options": {
        "packageName": "client",
        "distDir": "./dist/packages/client",
        "entryPoints": [
          "./index.ts"
        ],
        "ignorePatterns": ["@nestjs/", "@unique-nft/sdk", "@unique-nft/accounts"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/client/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/client"],
      "options": {
        "jestConfig": "packages/client/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
