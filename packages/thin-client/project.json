{
  "root": "packages/thin-client",
  "sourceRoot": "packages/thin-client/src",
  "targets": {
    "build": {
      "executor": "./tools/executors/lib-builder:run-build",
      "options": {
        "packageName": "thin-client",
        "distDir": "./dist/packages/thin-client",
        "entryPoints": [
          "./index.ts",
          "./src/index.ts"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/thin-client/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/thin-client"],
      "options": {
        "jestConfig": "packages/thin-client/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
