{
  "name": "unique-sdk",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "dev": "nx serve",
    "build": "nx build",
    "build:sdk": "nx build sdk",
    "build:accounts": "nx build accounts",
    "build:web": "nx build web",
    "build:client": "nx build client",
    "test": "nx run-many --target=test --all",
    "test:sdk": "nx test sdk",
    "test:web": "nx test web",
    "test:accounts": "nx test accounts",
    "test:client": "nx test client",
    "lint": "nx run-many --target=lint --all",
    "lint:fix": "eslint ./packages/** --fix",
    "start": "node -r source-map-support/register ./dist/packages/web/main.js",
    "legacy-postinstall": "npm run fix:unique:nft:types && npm run fix:quartz:nft:types && npm run fix:opal:nft:types && npm run husky:install",
    "fix:unique:nft:types": "find ./node_modules/@unique-nft/unique-mainnet-types/ -type f -name '*.ts' ! -name '*.d.ts' -type f -delete",
    "fix:quartz:nft:types": "find ./node_modules/@unique-nft/quartz-mainnet-types/ -type f -name '*.ts' ! -name '*.d.ts' -type f -delete",
    "fix:opal:nft:types": "find ./node_modules/@unique-nft/opal-testnet-types/ -type f -name '*.ts' ! -name '*.d.ts' -type f -delete",
    "husky:install": "husky install",
    "tools:generate:docs": "ts-node tools/generate-docs.ts",
    "generate-types:client": "npx swagger-typescript-api -p .. -o ./packages/client/src/types -n api.ts --no-client -p"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/platform-express": "^8.0.0",
    "@nestjs/swagger": "^5.2.1",
    "@polkadot/api": "^8.6.2",
    "@polkadot/extension-dapp": "^0.44.1",
    "@polkadot/ui-keyring": "^2.8.2",
    "@sentry/node": "^7.6.0",
    "@sentry/tracing": "^7.6.0",
    "@types/express": "^4.17.13",
    "@unique-nft/api": "^0.1.5",
    "@unique-nft/opal-testnet-types": "0.5.3",
    "@unique-nft/quartz-mainnet-types": "0.6.1",
    "@unique-nft/unique-mainnet-types": "0.6.3",
    "cache-manager": "^4.0.1",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "ethers": "^5.6.9",
    "file-type": "16.5.4",
    "ipfs-http-client": "53.0.1",
    "protobufjs": "^6.11.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.0.0",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^4.3.0",
    "tslib": "^2.0.0",
    "yauzl": "^2.10.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@nrwl/angular": "^14.1.5",
    "@nrwl/cli": "13.10.2",
    "@nrwl/eslint-plugin-nx": "13.10.2",
    "@nrwl/jest": "13.10.2",
    "@nrwl/js": "13.10.2",
    "@nrwl/linter": "13.10.2",
    "@nrwl/nest": "^13.10.2",
    "@nrwl/node": "13.10.2",
    "@nrwl/workspace": "13.10.2",
    "@types/cache-manager": "^4.0.0",
    "@types/jest": "27.0.2",
    "@types/node": "16.11.7",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "~5.18.0",
    "@typescript-eslint/parser": "~5.18.0",
    "axios": "^0.27.2",
    "eslint": "~8.12.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.1",
    "injection-js": "^2.4.0",
    "jest": "27.2.3",
    "lint-staged": "^13.0.0",
    "ng-packagr": "^13.3.1",
    "nx": "13.10.2",
    "postcss-import": "^14.1.0",
    "prettier": "^2.5.1",
    "rollup": "^2.75.5",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-dts": "^4.2.2",
    "rollup-plugin-esbuild": "^4.9.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "supertest": "^6.2.2",
    "swagger-typescript-api": "^9.3.1",
    "ts-jest": "27.0.5",
    "typescript": "~4.6.2"
  },
  "lint-staged": {
    "*.ts": "eslint --cache"
  }
}
